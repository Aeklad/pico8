pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
p = {
	x = 16,
	y = 16,
	sp = 240,
	flp = 1,
	spd = 1
	}
	
	poke(0x5f2c, 3)
	
	fc = 0
	t = 1
	
function _update()

	cls()
	t += 1
	
	if btn(0) then
		p.x -= p.spd
	end

	
	if btn(1) then
		p.x += p.spd
	end
	
	if btn(2) then
		p.y -= p.spd
	end


	if btn(3) then
		p.y += p.spd
	end

	if t % 5 == 0 then
		fc += 1
		
		if fc > 3 then
			fc = 0
		end
		
	end	
	
	spr(p.sp + fc, p.x, p.y,1 , 1, 10)
end	
