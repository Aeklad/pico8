 --updates
 function update_game()
  if btnp(⬅️) then
   p_x-=1
  end
  if btnp(➡️) then
   p_x+=1
  end
  if btnp(⬆️) then
   p_y-=1
  end
  if btnp(⬇️) then
   p_y+=1
  end
 end
 
 function update_gameover()
 end
-->8
--draws
 function draw_game()
 	cls(0)
    map()
    palt(0,false)
    spr(240,p_x*8,p_y*8)
 end
 
 function draw_gameover()
 end
