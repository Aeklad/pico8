pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
cls()
dr=3.14/180
function wait(a)
 for i=1,a do
  flip()
 end
end
function _draw()
for r=0,108,5 do
 for t= 0,108,2 do
  x=r*cos(t*dr)*.5
  y=r*sin(r*dr)*.3
  z=r*sin(t*dr)*.5
  xp=x+(cos(15*dr)*z)
  yp=y+(sin(15*dr)*z)
  xp=xp+60
  yp=50-yp
  pset(xp,yp,1)
  wait(1)
 end
end
end
			
