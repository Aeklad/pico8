pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
cls()
x=63
y=63
r = 10
function _update()
 for a = 0,1,.01 do
  ptx = x+r*cos(a)
  pty = y+r*sin(a)
 end
end
function _draw()
 pset(ptx,pty,8)
end
